{
    "manifest_version": 2,
    "name": "EcoSure",
    "description": "Shopping sustainably.",
    "version": "0.1",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "Untitled_Artwork.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
}
